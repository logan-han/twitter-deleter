{
  "name": "twitter-deleter",
  "version": "1.0.0",
  "description": "Delete tweets for free",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/jest --forceExit --verbose",
    "test:watch": "NODE_ENV=test node_modules/.bin/jest --watch",
    "test:coverage": "NODE_ENV=test node_modules/.bin/jest --coverage --forceExit",
    "format": "node_modules/.bin/prettier --write '**/*.{js,css,json,md}'"
  },
  "author": "Logan Han <logan@han.life>",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.666.0",
    "adm-zip": "^0.5.10",
    "async": "^0.9.2",
    "aws-sdk": "^2.1331.0",
    "body-parser": "^1.20.1",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formidable": "^3.5.4",
    "fs-extra": "^11.1.0",
    "multer": "^1.4.5-lts.1",
    "twitter-api-v2": "^1.17.2",
    "node-fetch": "^3.3.2",
    "crypto": "^1.0.1",
    "qs": "^6.14.0",
    "serverless-http": "^3.1.1",
    "express-rate-limit": "^7.5.1"
  },
  "devDependencies": {
    "jest": "^28.1.3",
    "prettier": "^2.8.4",
    "should": "^13.2.3",
    "supertest": "^6.3.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!coverage/**",
      "!node_modules/**",
      "!tests/setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "testTimeout": 10000,
    "silent": false,
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
